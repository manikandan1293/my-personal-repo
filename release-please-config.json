{
  "release-type": "python",
  "packages": {
    ".": {
      "package-name": "my-personal-repo",
      "extra-files": [
        {
          "type": "generic",
          "path": "__version__.py",
          "search": "__version__ = \"",
          "replace": "__version__ = \"${version}\""
        },
        {
          "type": "generic",
          "path": "README.md",
          "search": "Version: ",
          "replace": "Version: ${version}"
        }
      ]
    }
  }
}
